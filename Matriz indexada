#include <stdio.h>

int main() {
    float matriz[3][5];
    int linha, coluna;

 
    for (linha = 0; linha < 3; linha++) {
        for (coluna = 0; coluna < 5; coluna++) {
            scanf("%f", &matriz[linha][coluna]); 
        }
    }

    
    printf("\nMatriz:\n");
    for (linha = 0; linha < 3; linha++) {
        for (coluna = 0; coluna < 5; coluna++) {
            printf("%.2f ", matriz[linha][coluna]); 
        }
        printf("\n"); 
    }

    return 0;
}
